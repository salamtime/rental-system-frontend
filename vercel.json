{
  "headers": [
    {
      "source": "/assets/(.*).css",
      "headers": [{ "key": "Content-Type", "value": "text/css" }]
    },
    {
      "source": "/assets/(.*).js",
      "headers": [{ "key": "Content-Type", "value": "application/javascript" }]
    }
  ],
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
