{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/(.*\\.js|.*\\.css|.*\\.map|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.ico)", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
